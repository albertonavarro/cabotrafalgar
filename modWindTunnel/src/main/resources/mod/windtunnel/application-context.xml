<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <context:annotation-config/>
  
    <bean id="mod.windtunnel.mainscreen" class="com.navid.trafalgar.mod.windtunnel.WindTunnelMainScreen" autowire="byType"/>
    <bean id="mod.windtunnel.shipselector" class="com.navid.trafalgar.mod.windtunnel.ScreenSelectShip" autowire="byType"/>
    <bean id="mod.windtunnel.shipselectorgenerator" class="com.navid.trafalgar.mod.windtunnel.ShipSelectorInterfaceGenerator" autowire="byType" />
    <bean id="common.Configuration" class="com.navid.trafalgar.model.GameConfiguration" autowire="byType"  />

</beans>