<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
 
    <!-- builders -->
    <bean id="mod.counterclock.RotateSimpleWindBuilder" class="com.navid.trafalgar.model.builder.RotateSimpleWindBuilder" autowire="byType" />
    <bean id="mod.counterclock.ContextBuilder" class="com.navid.trafalgar.model.builder.ContextBuilder" autowire="byType" />
    <bean id="mod.counterclock.WaterBuilder2" class="com.navid.trafalgar.model.builder.WaterBuilder2" autowire="byType" />
    <bean id="mod.counterclock.SunBuilder" class="com.navid.trafalgar.model.builder.SunBuilder" autowire="byType" />
    <bean id="mod.counterclock.SkyBuilder" class="com.navid.trafalgar.model.builder.SkyBuilder" autowire="byType" />
    
    <bean id="common.input.Keyboard" class="com.navid.trafalgar.input.KeyboardCommandGenerator" autowire="byType" />
</beans>