<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <context:annotation-config/>
  
    <bean id="mod.windtunnel.selectshipcontroller" class="com.navid.trafalgar.mod.windtunnel.SelectShipScreenController" autowire="byType"/>
    <bean id="mod.windtunnel.selectshipgenerator" class="com.navid.trafalgar.mod.windtunnel.SelectShipScreenGenerator" autowire="byType" />
    <bean id="mod.windtunnel.selectcontrolcontroller" class="com.navid.trafalgar.mod.windtunnel.SelectControlsScreenController" autowire="byType" />
    <bean id="mod.windtunnel.selectcontrolgenerator" class="com.navid.trafalgar.mod.windtunnel.SelectControlsScreenGenerator" autowire="byType" />
    <bean id="mod.windtunnel.selectkeyscontroller" class="com.navid.trafalgar.mod.windtunnel.SelectKeyboardControlsScreenController" autowire="byType" />
    <bean id="mod.windtunnel.selectkeysgenerator" class="com.navid.trafalgar.mod.windtunnel.SelectKeyboardControlsScreenGenerator" autowire="byType" />
    
    

</beans>